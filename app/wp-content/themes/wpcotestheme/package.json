{
  "name": "wpcotestheme",
  "version": "1.0.0",
  "description": "Custom theme wordpress by WillyDev ",
  "scripts": {
    "start": "webpack --watch --config ./webpack-config/webpack.config.js --mode=development",
    "build": "webpack --config ./webpack-config/webpack.config.js --mode=production",
    "serve": "webpack serve --config ./webpack-config/webpack.config.js --mode=development",
    "eslint": "eslint \"./src/**/*.js\"",
    "eslint:fix": "eslint \"./src/**/*.js\" --fix",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "preclean": "rm ./*.css ./*.map ./*.rtl || true",
    "clean": "rm -rf ./assets/",
    "check:webpack": "webpack configtest ./webpack-config/webpack.config.js",
    "format": "prettier --end-of-line lf --write",
    "serve": "node ./serve.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@wordpress/eslint-plugin": "^9.3.0",
    "@wordpress/stylelint-config": "^19.1.0",
    "autoprefixer": "^10.4.1",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "2.5.1",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-webpack-plugin": "^3.1.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-merge": "^5.8.0"
  },
  "browser": {
    "[module-name]": false
  }
}
